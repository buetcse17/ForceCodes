<div class="my-container container mb-3">
    <!-- Logo and Prompt -->
    <div class="clearfix py-2 row mx-0">
        <!-- Logo -->
        <div class="ml-0 mr-auto py-2">
            <img src="images/ForceCodes_logo.png"  alt="ForceCodes" class="img-fluid my-logo">
        </div>
        <!-- Prompt -->
        <div class="ml-auto mr-0 my-auto my-site-font text-right">
            <% if(user === null){ %>
                <%- include('_non-user-prompt') %>
            <% } else { %>
                <%- include('_user-prompt') %>
            <% } %>
        </div>
    </div>

    <!-- nav bar -->
    <div class="card">
        <ul class="list-inline card-body p-2 my-nav">
            <li class="list-inline-item mr-2 ml-3">
                <a class="my-nav-link" href="/">Home</a>
            </li>
            <li class="list-inline-item mx-2">
                <a class="my-nav-link" href="/contests">Contests</a>
            </li>
            <li class="list-inline-item mx-2">
                <a class="my-nav-link" href="/problems">Problems</a>
            </li>
            <li class="list-inline-item mx-2">
                <a class="my-nav-link" href="/Users">Users</a>
            </li>
            <li class="list-inline-item mx-2">
                <a class="my-nav-link" href="/About">About</a>
            </li>
        </ul>
    </div>
</div>