<!-- profile -->
<div class="card border-secondary px-3 py-2">
    <div class="row">
        <div class="col-9">
            <div class="py-2">
                rank
            </div> 
            <div class="py-2 my-handle-header">
                handle
            </div> 
            <div class="text-muted pt-2">
                name, city, country
            </div> 
            <div class="text-muted pb-2">
                From organization
            </div> 
            <div class="py-2">
                contest rating: rating
            </div> 
            <div class="py-2">
                Friend of: count
            </div> 
            <div class="py-2">
                last visit: 
            </div> 
            <div class="py-2">
                registered:
            </div> 
            <div class="py-2">
                blog entries
            </div>
            <div class="py-2">
                talks
            </div>
        </div>
        <div class="col-3 mx-0 px-0 text-right">
            <div class="border text-center mx-3 my-2">
                <img src="/images/no-profile-picture.jpg" alt="no picture" class=" img-fluid m-4">    
            </div>
            
        </div>
    </div>
</div>